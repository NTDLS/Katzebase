--Get All Persons and Their Addresses (1-to-1 Relationship)
SELECT
	p.PersonID, p.FirstName, p.LastName, a.Street, a.City, a.State, a.ZipCode
FROM
	TestData:Person as p
INNER JOIN TestData:Address as A
	ON p.PersonID = a.PersonID

