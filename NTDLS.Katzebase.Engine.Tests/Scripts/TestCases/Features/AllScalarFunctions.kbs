	Concat('hello', ' ', 'world') as Concat1,
	Coalesce(null, null, 'hello', 'world') as Coalesce1,
	Trim('hello world', 'dhle') as Trim1,
	Trim('     hello world     ') as Trim2,
	IIF(IsGreater(Length ('Hello World'), 10), 'Is True', 'Is False') as IIF1
