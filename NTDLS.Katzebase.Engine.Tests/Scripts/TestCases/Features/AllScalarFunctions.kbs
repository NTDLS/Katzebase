	Sha1('Hello world') as Sha1,
	Sha256('Hello world') as Sha256,
	Sha512('Hello world') as Sha512,
	SubString('Hello cruel world', 6, 5) as SubString1,
	ToLower('Hello cruel world') as ToLower1,
