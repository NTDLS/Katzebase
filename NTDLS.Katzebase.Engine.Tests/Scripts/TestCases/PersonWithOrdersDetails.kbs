--Get All Person with Orders Details (Many-to-1 Relationship)
SELECT
	o.OrderID, p.FirstName, p.LastName, o.OrderDate, o.TotalAmount
FROM
	TestData:Person as p
INNER JOIN TestData:SalesOrder as o
	ON p.PersonID = o.PersonID

