--Get All Orders and Their Items (1-to-Many Relationship)
SELECT
	o.OrderID, o.OrderDate, oi.OrderItemID, oi.ProductName, oi.Quantity, oi.Price
FROM
	TestData:SalesOrder as o
INNER JOIN TestData:SalesOrderItem as oi
	ON oi.OrderID = o.OrderID

